<h2> Upload a new photo! </h2>

<%= form_for(@photo, {:html => { :multipart => true}}) do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <div class="form-group">
 <%= f.hidden_field :user_id, :value => current_user.id %>
 <%= f.submit(:class => "btn btn-danger") %>
</div>
<% end %>
